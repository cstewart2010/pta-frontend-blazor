    <PackageReference Include="PokemonTabletopAdventures.Models" Version="1.0.4" />
  </ItemGroup>

  <ItemGroup>
    <Using Include="PokemonTabletopAdventures.Web.Services" />
  </ItemGroup>

</Project>
